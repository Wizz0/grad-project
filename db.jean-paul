;
; BIND data file for local loopback interface
;
$TTL	604800
@	IN	SOA	jean-paul.str. root.jean-paul.str. (
			      2		; Serial
			 604800		; Refresh
			  86400		; Retry
			2419200		; Expire
			 604800 )	; Negative Cache TTL
;
@	IN	NS	jean-paul.str.
@	IN	A	192.168.1.1
srv	IN	A	192.168.1.1
web	IN	A	192.168.1.2
www	IN	CNAME	web
